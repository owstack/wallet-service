{
  "name": "@owstack/wallet-service",
  "description": "A service for multisignature HD wallets",
  "version": "0.2.7",
  "license": "MIT",
  "keywords": [
    "bitcoin",
    "btc",
    "bitcoin cash",
    "bch",
    "litecoin",
    "ltc",
    "multisignature",
    "wallet",
    "server"
  ],
  "repository": {
    "url": "git@github.com:owstack/wallet-service.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/owstack/wallet-service/issues"
  },
  "scripts": {
    "services": "./scripts/services.js",
    "start": "docker-compose up -d",
    "docker-rebuild": "docker-compose build",
    "stop": "docker-compose down",
    "coverage": "./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha -- --exit --reporter spec test",
    "lint": "./node_modules/.bin/eslint .",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha --exit",
    "coveralls": "NODE_ENV=test ./node_modules/.bin/istanbul cover ./node_modules/.bin/_mocha --report lcovonly -- --exit -R spec && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "postinstall": "npm run services create"
  },
  "owsNode": "ows-node",
  "dependencies": {
    "@owstack/bch-lib": "^0.4.2",
    "@owstack/btc-lib": "^0.1.24",
    "@owstack/key-lib": "^0.0.37",
    "@owstack/ltc-lib": "^0.1.23",
    "@owstack/ows-common": "^0.0.16",
    "@sendgrid/mail": "^6.3.1",
    "async": "^0.9.2",
    "body-parser": "^1.11.0",
    "compression": "^1.6.2",
    "config": "^3.0.1",
    "email-validator": "^1.0.1",
    "express": "^4.10.0",
    "express-rate-limit": "^2.6.0",
    "fs-extra": "^0.30.0",
    "json-stable-stringify": "^1.0.0",
    "locker": "^0.1.0",
    "locker-server": "^0.1.3",
    "mocha-lcov-reporter": "0.0.1",
    "moment": "^2.10.3",
    "mongodb": "^2.0.27",
    "morgan": "*",
    "mustache": "^2.1.0",
    "nodemailer": "^1.3.4",
    "nodemailer-sendgrid": "^1.0.3",
    "npmlog": "^0.1.1",
    "preconditions": "^2.2.3",
    "read": "^1.0.5",
    "request": "^2.53.0",
    "secp256k1": "^3.1.0",
    "sjcl": "^1.0.2",
    "socket.io": "^2.2.0",
    "socket.io-client": "^2.2.0",
    "sticky-session": "^0.1.0",
    "uuid": "*"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "coveralls": "^3.0.2",
    "eslint": "^5.13.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.3.0-beta1",
    "memdown": "^1.0.0",
    "mocha": "^5.2.0",
    "proxyquire": "^1.7.2",
    "sinon": "1.10.3",
    "supertest": "*",
    "tingodb": "^0.6.1"
  }
}
